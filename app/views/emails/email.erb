<%- model_class = Email -%>
<div class="page-header">
</div>
<div>
  <div class="email-form col-md-8">
    <%= render partial: 'emails/email_form', locals: { email: @email } %>
  </div>
  <div class="email-templates col-md-4">
    <% if params[:status] %>
      <%= render partial: 'emails/templates', locals: { template: @templates } %>
    <% end %>
  </div>
</div>